to isPrime :number
	make "i 2
	if :number = 0 [
		print :number "No
		make "i :number + 1
	]
	if :number = 1 [
		print :number "No
		make "i :number + 1
	]

	while [:i  * :i  <= :number] [
		if :number % :i  = 0 [
			print :number "No
			make "i :number
		]
		make "i  :i + 1
	]

	if :i < :number + 1 [
		print :number "Yes
	]

end

make "i 0
while [:i <= 50] [
	isPrime :i
	make "i :i + 1
]
